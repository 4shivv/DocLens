# DocuLens Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3000/api

# Application Configuration
VITE_APP_NAME=DocuLens
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Tax Document Analysis

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=application/pdf,image/jpeg,image/png,image/webp

# Processing Configuration
VITE_POLLING_INTERVAL=3000
VITE_MAX_POLLING_ATTEMPTS=60
VITE_REQUEST_TIMEOUT=30000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_ANIMATIONS=true
VITE_TOAST_DURATION=5000

# Development Configuration
VITE_DEV_API_DELAY=0
VITE_MOCK_PROCESSING_TIME=10000
VITE_ENABLE_LOGGING=true

# External Services (for future use)
# VITE_SENTRY_DSN=
# VITE_ANALYTICS_ID=
# VITE_HOTJAR_ID=
